<ExtensionManifest Version="5.0" ExtensionBundleId="com.dehats.threejsexample" ExtensionBundleVersion="1.0.0"
		ExtensionBundleName="Threejs Example" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.dehats.threejsexample" Version="1.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
            <Host Name="PHXS" Version="[15.0,15.9]" />
            <Host Name="PHSP" Version="[15.0,15.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="5.0" />
		</RequiredRuntimeList>
