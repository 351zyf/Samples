<ExtensionManifest Version="4.0" ExtensionBundleId="com.adobe.ScriptDictionary" ExtensionBundleVersion="1.0"
ExtensionBundleName="Script Dictionary" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.adobe.ScriptDictionary" Version="1.0">
			<HostList>
	            <Host Name="AUDT"/>
				<Host Name="PPRO"/>
				<Host Name="PRLD"/>
				<Host Name="AEFT"/>
	        </HostList>
	    </Extension>
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="AUDT" Version="9.2.0" />
			<Host Name="PPRO"/>
			<Host Name="PRLD"/>
			<Host Name="AEFT"/>
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="6.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>

	<DispatchInfoList>
		<Extension Id="com.adobe.ScriptDictionary">
			<DispatchInfo >
				<Resources>
					<MainPath>./index.html</MainPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
					<!--<StartOn>
                        <Event>com.adobe.csxs.events.ApplicationInitialized</Event>
                    </StartOn>-->
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>Script Dictionary</Menu>
					<Geometry>
						<Size>
							<Height>150</Height>
							<Width>150</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>
